{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "rootDir": "./src",
        "outDir": "./lib",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
        "strict": true,
        "noImplicitAny": true,
        "declaration": true
    },
    "include": [ "src/*.ts" ]
}
